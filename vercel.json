{
    "builds": [
        {
            "src": "base/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "medias/(.*)",
            "dest": "medias/$1"
        },
        {
            "src": "/(.*)",
            "dest": "base/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}
